/** 关于Yizo界面css */
#about .logo {
  text-align: center;
  margin-top: 20px;
}
#about .logo image {
  display: block;
  width: 80px;
  height: 80px;
  margin: 10px auto;
}
#about .content {
  width: 80%;
  margin: 0 auto;
  padding: 10px;
}
#about .content .item {
  padding: 10px 0;
  font-size: 16px;
}
#about .bottom {
  position: absolute;
  bottom: 10px;
  left: 0;
  right: 0;
  font-size: 12px;
  text-align: center;
  color: #aaa;
}
