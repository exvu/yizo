.loading{transform:rotate(45deg);position:relative;display:inline-block;animation:antRotate 1.2s infinite linear;width:14px;height:14px}.loading view{position:absolute;width:6px;height:6px;border-radius:100%;background-color:#108ee9;opacity:.3;transform-origin:50% 50%;animation:antSpinMove 1s infinite linear alternate;transform:scale(.75)}@keyframes antSpinMove{to{opacity:1}}@keyframes antRotate{to{transform:rotate(405deg)}}